int my_led = 5; //Define my LED pin

// the setup function runs once
void setup() {
  // initialize digital pin my_led as an output.
  pinMode(my_led, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {
  digitalWrite(my_led, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(1000);                       // wait for a second
  digitalWrite(my_led, LOW);    // turn the LED off (LOW is the voltage level)
  delay(1000);                       // wait for a second
}
