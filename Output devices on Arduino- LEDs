int LED1 =  3; //Declare 1st LED
int LED8 = 10; //Declare last LED


void setup() {
  Serial.begin(9600);
  for(int i= LED1; i <= LED8; i++) //Declare all LED in Output pin mode
    pinMode(i, OUTPUT);
}

void loop() {
  
  int lastLed = Serial.read();

  for(int i = LED1; i <= lastLed; i+=1)//Turn on Input number of LEDs
  {
    digitalWrite(i, HIGH);
    delay(2);
    digitalWrite(i, LOW);
  }

}
